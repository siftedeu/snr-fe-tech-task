{
  "name": "fe-interview-ui",
  "version": "0.1.0",
  "author": "sifted.eu",
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "dev-test": "NODE_ENV=test next dev",
    "build": "next build",
    "start": "next start -p $PORT",
    "start-test": "NODE_ENV=test next start -p 3000",
    "heroku-postbuild": "yarn build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:functional": "touch .env.test && env $(cat .env.test | grep -v \"#\" | xargs) CYPRESS_APP_URL=http://localhost:$PORT cypress open --browser chrome",
    "test:functional-headless": "touch .env.test && env $(cat .env.test | grep -v \"#\" | xargs) CYPRESS_APP_URL=http://localhost:$PORT cypress run --browser chrome",
    "check-eslint-config": "eslint-config-prettier .eslintrc",
    "lint": "eslint ./ --ext .js,jsx,ts,tsx --ignore-path .gitignore --cache"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.7",
    "@hookform/resolvers": "^2.9.10",
    "build": "^0.1.4",
    "classnames": "^2.3.2",
    "cypress-pipe": "^2.0.0",
    "dayjs": "^1.11.7",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "markdown-to-jsx": "^7.1.8",
    "next": "^13.0.0",
    "next-images": "^1.8.4",
    "raf": "^3.4.1",
    "react": "^18.2.0",
    "react-datocms": "^3.1.4",
    "react-dom": "^18.2.0",
    "react-icons": "^4.7.1",
    "react-inlinesvg": "^3.0.1",
    "react-intersection-observer": "^9.4.1",
    "react-medium-image-zoom": "^4.4.3",
    "react-player": "^2.11.0",
    "react-responsive": "^9.0.2",
    "react-transition-group": "^4.4.5",
    "react-use": "^17.4.0",
    "tailwindcss-aspect-ratio": "^3.0.0",
    "tailwindcss-gradients": "^3.0.0",
    "tailwindcss-pseudo-elements": "^2.0.0",
    "vanilla-lazyload": "^17.8.3"
  },
  "devDependencies": {
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/js-cookie": "^3.0.2",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "autoprefixer": "^10.4.13",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "cypress": "^12.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.4.0",
    "jest-dom": "^4.0.0",
    "jest-next-dynamic": "^1.0.1",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": ">=13.0.3",
    "next-router-mock": "^0.7.5",
    "postcss": "^8.4.20",
    "prettier": "2.2.0",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "prop-types": "^15.8.1",
    "superagent": "^8.0.6",
    "tailwindcss": "^3.2.4",
    "timezone-mock": "^1.3.6",
    "typescript": "^4.9.4",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn check-eslint-config && yarn lint"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix --cache",
      "prettier --write"
    ],
    "*.{json,css,.module.css}": [
      "prettier --write"
    ]
  },
  "volta": {
    "node": "16.10.0",
    "yarn": "1.22.19"
  }
}
